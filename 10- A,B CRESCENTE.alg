Algoritmo "DOIS_VALORES"

Var
   A,B: INTEIRO
Inicio
      ESCREVAl("DIGITE DOIS VALORES ")
      ESCREVA("DIGITE O PRIMEIRO VALOR: ")
      LEIA(A)
      ESCREVA("DIGITE O SEGUNDO VALOR: ")
      LEIA(B)

             SE A>B ENTAO
             ESCREVA ("A SEQUENCIA É: ", B, A)
                     SENAO
                     ESCREVA ("A ORDEM É: ",A,",",B)
             FIMSE


Fimalgoritmo