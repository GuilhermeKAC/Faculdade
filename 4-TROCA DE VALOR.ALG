Algoritmo "TROCA"

Var
   A, B, AUXILIAR: REAL

Inicio
      ESCREVA("Digite o valor de A: ")
      LEIA(A)
      ESCREVA("Digite o valor de B: ")
      LEIA(B)
      
             AUXILIAR:= A
             A:= B
             B:= AUXILIAR

                 ESCREVAL("VALOR A:",A)
                 ESCREVAL("         ")
                 ESCREVAL("VALOR B:",B)


Fimalgoritmo